num1 = 1.99
num2 = 2.01
suma = num1+num2
print('el resultado de la suma de {0} y {1}: {2} ' .format(num1, num 2, suma))
print('el resultado de la suma de {0} y {1}: {2} '.format(num1, num 2, suma))
suma = num1+num2
print('el resultado de la suma de {0} y {1}: {2} '.format(num1, num2, suma))
mult=num1*num2
print('el resultado de la multiplicación de {0} y {1}: {2} '.format(num1, num2, mult))
div=num1/num2
print('el resultado de la división de {0} y {1}: {2} '.format(num1, num2, div))
